<h1>Criar Vaga</h1>

<%= form_for @vaga do |f| %>
  <%= render 'shared/errors', object: @vaga %>

  <%= f.label :titulo %>
  <%= f.text_field :titulo %>

  <%= f.label :descricao %>
  <%= f.text_area :descricao %>

  <%= f.label :salario %>
  <%= f.text_field :salario %>

  <%= f.submit 'Criar Vaga' %>
<% end %>
